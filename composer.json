{
    "name": "wucdbm/symfony-form-types",
    "type": "library",
    "description": "A custom set of Symfony Form Types",
    "keywords": ["symfony form", "ip 2 long", "entity hidden type"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/orm": "~2.8",
        "symfony/form": "~5.0"
    },
    "require-dev": {
        "wucdbm/php-cs-fixers": "^0.3.0"
    },
    "autoload": {
        "psr-4": { "Wucdbm\\SymfonyFormTypes\\": "src" }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.4.0"
        }
    },
    "license": "MIT"
}
